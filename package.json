{
  "name": "angular-xmlrpc",
  "version": "2.0.0",
  "description": "AngularJS module for XML-RPC communication",
  "main": "dist/xmlrpc.min.js",
  "scripts": {
    "lint": "tslint 'src/**/*' && echo OK",
    "prejs-compile": "rm -rf dist/",
    "js-compile": "tsc",
    "build:bundle": "webpack -p --progress --profile --bail",
    "build:all": "npm run js-compile && npm run build:bundle && echo \"Build completed.\"",
    "karma": "karma start karma.conf.js",
    "test": "npm run karma"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ITrust/angular-xmlrpc"
  },
  "keywords": [
    "angular",
    "angularjs",
    "xmlrpc",
    "angular-xmlrpc",
    "communication"
  ],
  "authors": [
    {
      "name": "Julien Chakra"
    },
    {
      "name": "Vincent Alquier"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ITrust/angular-xmlrpc/issues"
  },
  "devDependencies": {
    "@types/angular": "^1.5.8",
    "@types/lodash": "^4.14.45",
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.55",
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.16.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.2.0",
    "typescript": "^2.1.6",
    "webpack": "^1.14.0"
  }
}
